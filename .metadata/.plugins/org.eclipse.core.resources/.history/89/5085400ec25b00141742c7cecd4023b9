/***************************************************************
 * @title String Numbers
 * @author Nathan Baum (nbaum510@gmail.com)
 * @date 10/24/2014
 * 
 * Precondition: User inputs string with numbers separated by a space.
 * Postcondition: Prints the numbers in ascending order by value.
 **************************************************************/
package lab7Package;
import java.util.Scanner;

public class stringNums {

	public static void main(String[] args) {
		String uIn, returnStr;
		int num=0, tens=0, bottom;
		
		Scanner scan = new Scanner(System.in);
		
		uIn = scan.nextLine();
		
		for(int i=uIn.length()-1; i>=0; i--){
			num = num+(uIn.charAt(i)-30)*(10^tens);
			
			if(uIn.charAt(i) == ' '){
				try{
					if(num<bottom) bottom = num;
				}
				catch(Exception e){
					bottom = num;
				}
				num = 0;
				tens = 0;
			}
		}
		
		returnStr = Integer.toString(bottom);
		/*for(int i=uIn.length()-1; i>=0; i--){
			
		}*/
		
		System.out.println("lowest number: " + returnStr);
		
		scan.close();
	}

}
